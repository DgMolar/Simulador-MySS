  "description": "Sistema para modelar las incidencias de la diabetes tipo 2 en la poblacion de Nuevo León, México, basado en la prevalencia de la obesidad.",
  "main": "src/main/main.js",
  "scripts": {
    "start": "electron src/main/main.js",
    "build": "electron-builder",
    "pack": "electron-builder --dir",
    "dist": "electron-builder --win --x64"
  },
  "author": "Equipo-3(MySS)",
  "license": "MIT",
  "devDependencies": {
    "electron": "^27.0.4",
    "electron-builder": "^24.6.4"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.4.2",
    "bootstrap": "^5.3.2",
    "chart.js": "^4.4.0",
    "csvtojson": "^2.0.10",
    "date-fns": "^2.30.0",
    "jquery": "^3.7.1",
    "regression": "^2.0.1",
    "sqlite3": "^5.1.6"
  },
  "build": {
    "productName": "Simulador de incidencias-v2",
    "appId": "diego.molardlcr@uanl.edu.mx",
    "directories": {
      "output": "dist"
    },
    "win": {
      "target": "NSIS",
      "icon": "./logo-app-prueba.png"
    }
  }
